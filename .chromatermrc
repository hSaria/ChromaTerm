%CONFIG     {COMMAND CHAR} {%}
%CONFIG     {CONVERT META} {OFF}
%CONFIG     {HIGHLIGHT} {ON}

/* IPv4 */
%HIGHLIGHT  {(?i)((?P<ip>25[0-5]|(2[0-4]|[0-1]?\d)?\d)\.){3}(?P>ip)(/(3[0-2]|[0-2]?\d))?} {cyan} {5}

/* IPv6 */
%HIGHLIGHT  {(?i)(::(ffff(:0{1,4})?:)?((?P<ip>25[0-5]|(2[0-4]|[0-1]?\d)?\d)\.){3}(?P>ip)|((?P<h>[\da-f]{1,4}):){1,4}:((?P>ip)\.){3}(?P>ip)|((?P>h):){7}(?P>h)|(?P>h):((:(?P>h)){1,6})|((?P>h):){1,2}(:(?P>h)){1,5}|((?P>h):){1,3}(:(?P>h)){1,4}|((?P>h):){1,4}(:(?P>h)){1,3}|((?P>h):){1,5}(:(?P>h)){1,2}|((?P>h):){1,6}:(?P>h)|:((:(?P>h)){1,7}|:)|((?P>h):){1,7}:)(%[\da-z]+)?(/(12[0-8]|1[0-1]\d|\d{1,2}))?} {magenta} {3}

/* MAC */
%HIGHLIGHT  {(?i)((?P<h>[\da-f])(?P>h):){5}(?P>h){2}|((?P>h){4}\.){2}(?P>h){4}} {yellow} {5}

/* Password or password */
%HIGHLIGHT  {(?i)password} {red} {5}

/* Date in MMM DD hh:mm:ss format */
%HIGHLIGHT  {(?i)(jan|feb|mar|apr|may|jun|jul|aug|sep|oct|nov|dec)\s((\s\d)|(3[0-1]|([0-2]\d)))\s((2[0-3])|[0-1]\d):[0-5]\d:[0-5]\d} {<fca>} {5}
